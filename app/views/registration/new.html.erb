
      <h1>New user</h1>

      <%= form_for @user, url: {action: "create"} do |f| %>
        <%= f.label :name %>
        <%= f.text_field :name, id: "user_name" %><br />
        <%= f.label :height %>
        <%= f.text_field :height, id: "user_height" %><br />
        <%= f.label :happiness %>
        <%= f.text_field :happiness, id: "user_happiness" %><br />
        <%= f.label :nausea %>
        <%= f.text_field :nausea, id: "user_nausea" %><br />
        <%= f.label :tickets %>
        <%= f.text_field :tickets, id: "password" %><br />
        <%= f.label :password %>
        <%= f.password_field :password, id: "password" %><br />
        <%= f.check_box :admin, id: "user_admin"%><br/>
        <%= f.submit "Create User" %>
      <% end %>

  

<a href="/users">Back</a>
